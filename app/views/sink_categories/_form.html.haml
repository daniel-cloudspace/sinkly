= form_for @sink_category do |f|
  -if @sink_category.errors.any?
    #error_explanation
      %h2= "#{pluralize(@sink_category.errors.count, "error")} prohibited this sink_category from being saved:"
      %ul
        - @sink_category.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :photo_file_name
    = f.text_field :photo_file_name
  .field
    = f.label :photo_content_type
    = f.text_field :photo_content_type
  .field
    = f.label :photo_file_size
    = f.number_field :photo_file_size
  .field
    = f.label :photo_udpated_at
    = f.datetime_select :photo_udpated_at
  .field
    = f.label :user_id
    = f.number_field :user_id
  .actions
    = f.submit 'Save'
